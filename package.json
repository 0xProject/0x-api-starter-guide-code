{
    "name": "cfl-instadapp",
    "version": "0.0.1",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "compile": "sol-compiler",
        "generate_contract_wrappers": "abi-gen --abis ${npm_package_config_abis} --template ./node_modules/@0x/abi-gen-templates/contract.handlebars --partials './node_modules/@0x/abi-gen-templates/partials/**/*.handlebars' --output generated-wrappers --backend ethers",
        "build": "yarn compile && yarn generate_contract_wrappers",
        "test": "ts-node test/test.ts"
    },
    "config": {
        "abis": "./generated-artifacts/@(LiquidityRequiringContract).json"
    },
    "dependencies": {
        "0x.js": "^7.0.2",
        "@0x/contracts-exchange-forwarder": "^3.0.12",
        "@0x/contracts-exchange-libs": "^3.0.5",
        "@0x/asset-swapper": "^2.0.0",
        "@0x/utils": "^4.5.0",
        "@0x/web3-wrapper": "^6.0.10"
    },
    "devDependencies": {
        "@0x/abi-gen": "^4.1.0",
        "@0x/abi-gen-templates": "^2.4.1",
        "ts-node": "^8.3.0",
        "typescript": "^3.5.3",
        "@0x/sol-compiler": "^3.1.12"
    }
}
